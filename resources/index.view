<svg viewport-fill="black">
  <text id="recentSleepList" font-size="20" font-family="System-Bold" text-length="40" text-anchor="middle" x="50%"
    y="50%-80" fill="gray">+3.5h / -3.5h / +3.5h</text>
  <text id="clockText" font-size="80" font-family="System-Bold" text-length="32" text-anchor="middle" x="50%" y="50%"
    fill="white">00:00</text>
  <text id="remainingTimeText" font-size="40" font-family="System-Bold" text-length="32" text-anchor="middle" x="50%"
    y="50%+60" fill="white">00:00:00</text>
  <text id="lastSleepText" font-size="28" font-family="System-Bold" text-length="32" text-anchor="middle" x="50%"
    y="50%+100" fill="gray">Last Sleep: 14.5 hrs</text>
</svg>